{
  "name": "mtrdesign/krait",
  "description": "Krait provides an easy way to create Ajax Datatables.",
  "keywords": ["datatable", "laravel", "filtering", "pagination", "ajax"],
  "license": "MIT",
  "homepage": "https://github.com/mtrdesign/krait",
  "support": {
    "issues": "https://github.com/mtrdesign/krait/issues",
    "source": "https://github.com/mtrdesign/krait"
  },
  "scripts": {
    "check-style": "pint --test -v",
    "fix-style": "pint -v",
    "dry-fix-style": "pint --dirty -v"
  },
  "authors": [
    {
      "name": "MTR Design",
      "homepage": "http://mtr-design.com/",
      "email": "office@mtr-design.co.uk",
      "role": "Maintainer"
    }
  ],
  "require": {
    "php": "^8.2"
  },
  "autoload": {
    "psr-4": {
      "MtrDesign\\Krait\\": "krait/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MtrDesign\\Krait\\Tests\\": "krait/tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "MtrDesign\\Krait\\KraitServiceProvider"
      ]
    }
  },
  "require-dev": {
    "laravel/pint": "^1.16"
  }
}
